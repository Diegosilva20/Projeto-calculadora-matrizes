{
  "headers": [
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/matrizcalculator.com",
      "destination": "https://www.matrizcalculator.com",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://www.matrizcalculator.com/$1",
      "has": [
        {
          "type": "host",
          "value": "matrizcalculator.com"
        }
      ],
      "permanent": true
    },
    {
      "source": "/tutorial/1",
      "destination": "/tutorial/determinante-de-matrizes",
      "permanent": true
    },
    {
      "source": "/tutorial/2",
      "destination": "/tutorial/matriz-inversa",
      "permanent": true
    },
    {
      "source": "/tutorial/3",
      "destination": "/tutorial/escalonamento-gauss-jordan",
      "permanent": true
    },
    {
      "source": "/tutorial/4",
      "destination": "/tutorial/matriz-transposta",
      "permanent": true
    },
    {
      "source": "/tutorial/5",
      "destination": "/tutorial/multiplicacao-de-matrizes",
      "permanent": true
    },
    {
      "source": "/tutorial/6",
      "destination": "/tutorial/sistemas-lineares",
      "permanent": true
    }
  ]
}